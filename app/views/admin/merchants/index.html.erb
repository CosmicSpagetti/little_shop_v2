<h1>All Merchants</h1>

<% @merchants.each do |merchant| %>
  <div id="merchant-id-<%= merchant.id %>">
    <h2>Name: <%= link_to merchant.name, admin_merchant_path(merchant) %></h2>
    <p>Email: <%= merchant.email %></p>
    <p>Street address: <%= merchant.address %></p>
    <p>City: <%= merchant.city %></p>
    <p>State: <%= merchant.state %></p>
    <p>Zip code: <%= merchant.zip %></p>
    <p>Account Created: <%= Date.strptime(merchant.created_at.to_s) %></p>
  </div>
<% end %>
