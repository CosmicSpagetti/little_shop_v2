<h1>Merchants</h1>
<% @active_merchants.each do |merchant| %>
<div id="merchant-id-<%= merchant.id %>">
  <h3>Merchant: <%= merchant.name %></h3>
  <ul>
    <<li>Location: <%= merchant.city %>, <%= merchant.state %>.</li>
    <<li>Account Created: <%= Date.strptime(merchant.created_at.to_s) %></li> 
  </ul>
</div>
<% end %>
